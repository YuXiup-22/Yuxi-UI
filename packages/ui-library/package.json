{
  "name": "@yuxi-ui/ui-library",
  "version": "1.0.5",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@yuxi-ui/button": "workspace:*",
    "@yuxi-ui/click": "workspace:*",
    "@yuxi-ui/config-provider": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "5.8.2",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "@vitejs/plugin-react": "^4.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
